@model IEnumerable<WebApp.Product>
@{
    ViewBag.Title = "SuggestionSystems";
}

<h2>Suggestion Systems</h2><br/>

<h3>Knn algorithm</h3>
<div class="row"> 
    <div class="col-md-12" >
        <table class="table">
            <tr>
                <td>#</td>
                <td>Name</td>
                <td>Brand</td>
                <td>Model</td>
                <td>Price</td>
                <td></td>
            </tr>
            @{
                int i = 0;

            }
            @foreach (var item in Model)
            {
                i++;
                <tr>
                    <td>@i</td>
                    <td>@item.Name</td>
                    <td>@item.Brand</td>
                    <td>@item.Model</td>
                    <td>@item.Price</td>
                </tr>
            }
        </table>
    </div>
</div>